{
  "version": 2,
  "builds": [
    {
      "src": "back-end.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/icons/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "back-end.py"
    }
  ]
}
